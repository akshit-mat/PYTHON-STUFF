elif(A==8):
    h="x"
    a,b,c,d,e,f,g,i=" "," "," "," "," "," "," "," "
elif(A==9):
    i="x"
    a,b,c,d,e,f,g,h=" "," "," "," "," "," "," "," "
print("\n",a,"|",b,"|",c,"\n-----------\n",d,"|",e,"|",f,"\n-----------\n",g,"|",h,"|",i)

B=int(input("player2 enter position: "))
if(B==A):
    print("ERROR")
    sys.exit()
elif(B==1):
    a="O"
elif(B==2):
    b="O"
