import sys

#first turn --------------------------------------------------------------------------------------------------------------------

print("computer plays: ")
c_turn1=3
c="o"

a,b,d,e,f,g,h,i=" "," "," "," "," "," "," "," "
print("\n",a,"|",b,"|",c,"\n-----------\n",d,"|",e,"|",f,"\n-----------\n",g,"|",h,"|",i)

#second turn -------------------------------------------------------------------------------------------------------------------

p_turn2=int(input("player1 enter position: "))
